  <defaultDamageType>EM</defaultDamageType>
  <minimumAmmoCharges>1</minimumAmmoCharges> <!-- If the weapon has less then minimumAmmoCharges, the weapon is forced to reload before attacking a new target -->
  <capacitorInjectorToLoad>0</capacitorInjectorToLoad>   <!-- Amount of cap booster you want to carry with you, when using a cap injector in fitting-->
  <weaponGroupId>509</weaponGroupId><!-- Weapon group (508 = Siege, 506 = Cruise, 771 = HAM, etc of the Launcher / Guns / Lasors, NOT the Ammo, *the weapon*) -->
  <speedNPCFrigatesShouldBeIgnoredByPrimaryWeapons>100</speedNPCFrigatesShouldBeIgnoredByPrimaryWeapons> <!--  BOTH distance and speed have to be met before the NPC Frigate will be removed from the PrimaryWeapons PriorityTarget List... (under the distance, over the speed) -->
  <distanceNPCFrigatesShouldBeIgnoredByPrimaryWeapons>99999999</distanceNPCFrigatesShouldBeIgnoredByPrimaryWeapons> <!--  If you wish to effectively turn this off you can set the speedTargetsShouldBeIgnoredByMainWeapons to a very high number like: 9999999 -->
  <armLoadCapBoosters>false</armLoadCapBoosters>
  <insideThisRangeIsHardToTrack>0</insideThisRangeIsHardToTrack> <!-- If an NPC gets inside this range do not consider it within optimal range anymore (targets within optimal are chosen first) -->
  <maximumHighValueTargets>4</maximumHighValueTargets><!-- default is 4 --> <!-- Maximum number of targets per value -->
  <maximumLowValueTargets>3</maximumLowValueTargets><!-- default is 3 -->

  <!-- MOVEMENT SETTINGS -->
  <avoidBumpingThings>true</avoidBumpingThings> <!-- in missions while moving and when traveling attempt to avoid being too close to certain objects by orbiting those objects to get way from them if needed -->
  <speedTank>true</speedTank> <!-- When SpeedTank is enabled, Combat-module will try to orbit ships at orbitDistance -->
  <orbitDistance>5000</orbitDistance> <!-- The range used by SpeedTank -->
  <orbitStructure>false</orbitStructure> <!--  When SpeedTank is true this optional setting allows you to orbit the gate rather than the target -->
  <minimumPropulsionModuleDistance>1000</minimumPropulsionModuleDistance> <!-- Propulsion mod settings, do not activate AB/MWD unless these settings are matched -->
  <minimumPropulsionModuleCapacitor>55</minimumPropulsionModuleCapacitor> <!-- if you are not speed tanking then this value is used to determine where you are 'in range' if it is set. If you want your maximum range to be 'in range' leave this at 0: if you are in a gunboat you probably want this set to your optimal range-->
  <optimalRange>0</optimalRange>
  
  <!-- TANK SETTINGS -->
  <activateRepairModules>75</activateRepairModules> <!-- Activate shield booster or armor rep when shields/armor drop below this percentage -->
  <deactivateRepairModules>95</deactivateRepairModules> <!-- Deactivate shield booster or armor rep when shields/armor are above this percentage -->
  <injectcapperc>10</injectcapperc> <!-- Use cap injector when below this percentage -->
  
  <!-- PANIC SETTINGS -->
  <minimumShieldPct>20</minimumShieldPct>   <!-- Minimum amount of shields before the bot panics and warps to safety -->
  <minimumArmorPct>0</minimumArmorPct> <!-- Minimum amount of armor before the bot panics and warps to safety -->
  <minimumCapacitorPct>13</minimumCapacitorPct> <!-- Minimum amount of capacitor before the bot panics and warps to safety -->
  <safeShieldPct>0</safeShieldPct> <!-- Amount of shield that is seen as "safe" to return to the mission -->
  <safeArmorPct>0</safeArmorPct> <!-- Amount of armor that is seen as "safe" to return to the mission -->
  <safeCapacitorPct>65</safeCapacitorPct> <!-- Amount of capacitor that is seen as "safe" to return to the mission -->
  <safeSpotBookmarkPrefix>safespot</safeSpotBookmarkPrefix><!-- default is safespot: --> <!-- All safespot bookmarks will be prefixed by this string, default: safespot: -->
  
  <!-- DRONE SETTINGS -->
  <useDrones>false</useDrones> <!-- Use drones, disable on ships without a drone bay! -->
  <droneTypeId>2456</droneTypeId> <!-- Drone type id to load (set to 0 to not reload drones) -->
  <droneControlRange>44500</droneControlRange> <!-- Maximum drone control range -->
  <droneMinimumShieldPct>5</droneMinimumShieldPct>  <!-- Do not launch drones if below this shield percentage -->
  <droneMinimumArmorPct>0</droneMinimumArmorPct><!-- Do not launch drones if below this armor percentage -->
  <droneMinimumCapacitorPct>5</droneMinimumCapacitorPct> <!-- Do not launch drones if below this capacitor percentage -->
  <droneRecallShieldPct>5</droneRecallShieldPct> <!-- Recall drones if below this shield percentage -->
  <droneRecallArmorPct>0</droneRecallArmorPct> <!-- Recall drones if below this armor percentage -->
  <droneRecallCapacitorPct>10</droneRecallCapacitorPct> <!-- Recall drones if below this capacitor percentage -->
  <longRangeDroneRecallShieldPct>40</longRangeDroneRecallShieldPct> <!-- Recall drones if below this shield percentage and further away then half the drone control range -->
  <longRangeDroneRecallArmorPct>0</longRangeDroneRecallArmorPct> <!-- Recall drones if below this armor percentage and further away then half the drone control range -->
  <longRangeDroneRecallCapacitorPct>12</longRangeDroneRecallCapacitorPct> <!-- Recall drones if below this capacitor percentage and further away then half the drone control range -->
  <dronesKillHighValueTargets>false</dronesKillHighValueTargets> <!-- Drones normally kill low value targets (frigates) first. If you would prefer your drones to kill high value targets first set this to true -->
  
