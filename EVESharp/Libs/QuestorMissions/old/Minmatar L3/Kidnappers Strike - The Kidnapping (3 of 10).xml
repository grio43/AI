<?xml version="1.0" encoding="utf-8" ?>
<mission>
  <trytobring>Drazin's Keycard</trytobring> <!-- will attempt to bring, if item not avail, continue -->
  <pockets>
    <pocket id="0">
      <actions>
		<action name="ClearPocket"></action>
		<action name="Activate">
          <parameter name="target" value="Acceleration Gate" />
        </action>
      </actions>
    </pocket>
	<pocket id="1">
      <actions>
		<action name="ClearPocket"></action>
		<action name="Kill">
          <parameter name="Target" value="Drazin's Habitat" />
        </action>
		<action name="Loot"> <!-- this loots the field if LootEverything is true, if False it only loots item below then moves on --> 
          <parameter name="Item" value="Drazin Keycard" />
          <parameter name="Target" value="Cargo Container" />
        </action>
	    <action name="Activate">
          <parameter name="target" value="Acceleration Gate" />
        </action>
      </actions>
    </pocket>
	<pocket id="2">
      <actions>
		<action name="ClearPocket"></action>
		<action name="Loot"> <!-- this loots the field if LootEverything is true, if False it only loots item below then moves on --> 
        <parameter name="Item" value="Prisoners" />
        <parameter name="Target" value="Cargo Container" />
      </action>
		<action name="Done"/>
      </actions>
    </pocket>
  </pockets>
</mission>