<?xml version="1.0" encoding="utf-8"?>
<mission>
  <killSentries>true</killSentries>
  <pockets>
    <pocket id="0">
      <actions>
        <action name="WaitUntilTargeted">
          <parameter name="Timeout" value="30" />
        </action>
        <action name="ClearPocket" />
		<action name="AddWebifierByName"> <!-- Add this NPC by name to the list of known Webifiers-->
			<parameter name="Target" value="Drone Stasis Tower" />
		</action>
		<action name="MoveTo"> <!-- initiate movement to the "target" and fight only those NPCs that target you while you are on the way, process no other actions until you reach the target-->
			<parameter name="Target" value="Drone Stasis Tower" />
			<parameter name="Distance" value="30000" />
		</action>
        <action name="ClearPocket" />
        <action name="Activate">
          <parameter name="Target" value="Navigational Beacon" />
        </action>
      </actions>
    </pocket>
    <pocket id="1">
      <actions>
        <action name="WaitUntilTargeted">
          <parameter name="Timeout" value="30" />
        </action>
        <action name="Kill">
          <parameter name="Target" value="Infested Station" />
        </action>
		<action name="ClearPocket" />
        <action name="Done" />
      </actions>
    </pocket>
  </pockets>
</mission>